{
  "name": "speed-dial",
  "displayName": "Speed Dial",
  "description": "Jump quickly to your favorites files",
  "version": "0.1.0",
  "license": "MIT",
  "author": {
    "name": "Mariano Ibarra",
    "email": "marianoibarratesta@outlook.com",
    "url": "https://marianoibarra.com/"
  },
  "repository": {
    "type": "git",
    "url": "www.github.com/marianoibarra/speed-dial.git"
  },
  "engines": {
    "vscode": "^1.86.0"
  },
  "l10n": "./l10n",
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "speedDial",
          "name": "Speed dial"
        }
      ]
    },
    "keybindings": [
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad1",
        "args": "1"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad2",
        "args": "2"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad3",
        "args": "3"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad4",
        "args": "4"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad5",
        "args": "5"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad6",
        "args": "6"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad7",
        "args": "7"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad8",
        "args": "8"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad9",
        "args": "9"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad1",
        "args": "1"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad2",
        "args": "2"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad3",
        "args": "3"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad4",
        "args": "4"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad5",
        "args": "5"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad6",
        "args": "6"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad7",
        "args": "7"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad8",
        "args": "8"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad9",
        "args": "9"
      },
      {
        "command": "speed-dial.clear",
        "key": "ctrl+numpad_multiply ctrl+numpad_divide"
      }
    ],
    "commands": [
      {
        "command": "speed-dial.save",
        "title": "%command.save%"
      },
      {
        "command": "speed-dial.saveFromMenu",
        "title": "%command.saveFromMenu%",
        "icon": "$(file-symlink-file)"
      },
      {
        "command": "speed-dial.open",
        "title": "%command.open%"
      },
      {
        "command": "speed-dial.openFromMenu",
        "title": "%command.open%"
      },
      {
        "command": "speed-dial.refresh",
        "title": "%command.refresh%",
        "category": "Speed Dial",
        "icon": "$(refresh)"
      },
      {
        "command": "speed-dial.edit",
        "title": "%command.edit%",
        "icon": "$(edit)"
      },
      {
        "command": "speed-dial.remove",
        "title": "%command.remove%",
        "icon": "$(trash)"
      },
      {
        "command": "speed-dial.clear",
        "title": "%command.clear%",
        "category": "Speed Dial"
      },
      {
        "command": "speed-dial.openFromCommandPallete",
        "title": "%command.open%",
        "category": "Speed Dial"
      }
    ],
    "menus": {
      "commandPalette": [
        {
          "command": "speed-dial.openFromCommandPallete",
          "title": "%command.open%"
        },
        {
          "command": "speed-dial.save",
          "title": "%command.save%",
          "when": "false"
        },
        {
          "command": "speed-dial.saveFromMenu",
          "title": "%command.saveFromMenu%",
          "icon": "$(file-symlink-file)",
          "when": "false"
        },
        {
          "command": "speed-dial.open",
          "title": "%command.open%",
          "when": "false"
        },
        {
          "command": "speed-dial.openFromMenu",
          "title": "%command.open%",
          "when": "false"
        },
        {
          "command": "speed-dial.edit",
          "title": "%command.edit%",
          "icon": "$(edit)",
          "when": "false"
        },
        {
          "command": "speed-dial.remove",
          "title": "%command.remove%",
          "icon": "$(trash)",
          "when": "false"
        }
      ],
      "view/title": [
        {
          "command": "speed-dial.refresh",
          "when": "view == speedDial",
          "group": "navigation"
        }
      ],
      "view/item/context": [
        {
          "command": "speed-dial.edit",
          "when": "view == speedDial && viewItem == bookmark",
          "group": "inline"
        },
        {
          "command": "speed-dial.remove",
          "when": "view == speedDial && viewItem == bookmark",
          "group": "inline"
        },
        {
          "command": "speed-dial.openFromMenu",
          "when": "view == speedDial && viewItem == bookmark",
          "group": "1_actions@1"
        },
        {
          "command": "speed-dial.edit",
          "when": "view == speedDial && viewItem == bookmark",
          "group": "5_copy@2"
        },
        {
          "command": "speed-dial.remove",
          "when": "view == speedDial && viewItem == bookmark",
          "group": "5_copy@3"
        },
        {
          "command": "speed-dial.saveFromMenu",
          "when": "view == speedDial && viewItem == empty"
        },
        {
          "command": "speed-dial.saveFromMenu",
          "when": "view == speedDial && viewItem == empty",
          "group": "inline"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@types/vscode": "^1.86.0",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "@vscode/l10n-dev": "^0.0.33",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@vscode/l10n": "^0.0.18"
  }
}
