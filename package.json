{
  "name": "speed-dial",
  "displayName": "Speed Dial",
  "description": "Jump quickly to your favorites files",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:speed-dial.save-0",
    "onCommand:speed-dial.save-1",
    "onCommand:speed-dial.save-2",
    "onCommand:speed-dial.save-3",
    "onCommand:speed-dial.save-4",
    "onCommand:speed-dial.save-5",
    "onCommand:speed-dial.save-6",
    "onCommand:speed-dial.save-7",
    "onCommand:speed-dial.save-8",
    "onCommand:speed-dial.save-9",
    "onCommand:speed-dial.open-0",
    "onCommand:speed-dial.open-1",
    "onCommand:speed-dial.open-2",
    "onCommand:speed-dial.open-3",
    "onCommand:speed-dial.open-4",
    "onCommand:speed-dial.open-5",
    "onCommand:speed-dial.open-6",
    "onCommand:speed-dial.open-7",
    "onCommand:speed-dial.open-8",
    "onCommand:speed-dial.open-9"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "views": {
      "explorer": [
        {
          "id": "speedDial",
          "name": "Speed dial"
        }
      ]
    },
    "keybindings": [
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad0",
        "args": "0"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad1",
        "args": "1"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad2",
        "args": "2"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad3",
        "args": "3"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad4",
        "args": "4"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad5",
        "args": "5"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad6",
        "args": "6"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad7",
        "args": "7"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad8",
        "args": "8"
      },
      {
        "command": "speed-dial.save",
        "key": "ctrl+numpad_multiply ctrl+numpad9",
        "args": "9"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad0",
        "args": "0"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad1",
        "args": "1"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad2",
        "args": "2"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad3",
        "args": "3"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad4",
        "args": "4"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad5",
        "args": "5"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad6",
        "args": "6"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad7",
        "args": "7"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad8",
        "args": "8"
      },
      {
        "command": "speed-dial.open",
        "key": "ctrl+numpad9",
        "args": "9"
      },
      {
        "command": "speed-dial.removeAll",
        "key": "ctrl+numpad_multiply ctrl+numpad_divide"
      }
    ],
    "commands": [
      {
        "command": "speed-dial.save",
        "title": "Save bookmark",
        "category": "Workspace"
      },
      {
        "command": "speed-dial.saveFromMenu",
        "title": "Save current file to bookmark",
        "category": "Workspace",
        "icon": "$(file-symlink-file)"
      },
      {
        "command": "speed-dial.open",
        "title": "Open bookmark",
        "category": "Workspace"
      },
      {
        "command": "speed-dial.refresh",
        "title": "Refresh Speed Dial",
        "category": "Workspace",
        "icon": "$(refresh)"
      },
      {
        "command": "speed-dial.edit",
        "title": "Edit bookmark",
        "category": "Workspace",
        "icon": "$(edit)"
      },
      {
        "command": "speed-dial.remove",
        "title": "Remove bookmark",
        "category": "Workspace",
        "icon": "$(trash)"
      },
      {
        "command": "speed-dial.removeAll",
        "title": "Clear all Speed Dial",
        "category": "Workspace"
      }
    ],
    "menus": {
      "view/title": [
          {
              "command": "speed-dial.refresh",
              "when": "view == speedDial",
              "group": "navigation"
          }
      ],
      "view/item/context": [
        {
					"command": "speed-dial.edit",
					"when": "view == speedDial && viewItem == bookmark",
					"group": "inline"
				},
        {
					"command": "speed-dial.remove",
					"when": "view == speedDial && viewItem == bookmark",
          "group": "inline"
				},
        {
					"command": "speed-dial.open",
					"when": "view == speedDial && viewItem == bookmark"
				},
        {
					"command": "speed-dial.edit",
					"when": "view == speedDial && viewItem == bookmark"
				},
        {
					"command": "speed-dial.remove",
					"when": "view == speedDial && viewItem == bookmark"
				},
        {
					"command": "speed-dial.saveFromMenu",
					"when": "view == speedDial && viewItem == empty"
				},
        {
					"command": "speed-dial.saveFromMenu",
					"when": "view == speedDial && viewItem == empty",
          "group": "inline"
				}
      ]
  }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.4",
    "@vscode/test-electron": "^2.3.9"
  }
}
